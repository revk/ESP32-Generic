gpio	gfx.sck		//	GFX SCK
gpio	gfx.mosi	//	GFX MOSI
gpio	gfx.dc		//	GFX DC
gpio	gfx.rst		//	GFX RST
gpio	gfx.busy	//	GFX BUSY
gpio	gfx.ena		//	GFX ENA
u8	gfx.flip	//	GFX flip
